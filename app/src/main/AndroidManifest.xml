<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eheca.vintappexamenpractico">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".VintappExamenApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.Activities.MainActivity" />
        <activity
            android:name=".View.Activities.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.Activities.Proyectos"
            android:label="@string/title_activity_proyectos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".View.Activities.ConstruccionActivity"></activity>
    </application>

</manifest>